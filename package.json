    "angular",
    "minimal",
    "single-page",
    "frontend",
    "library"
  ],
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.4.1",
    "commitizen": "^2.4.6",
    "cz-conventional-changelog": "^1.1.5",
    "http-server": "^0.8.5",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "semantic-release": "^4.3.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
